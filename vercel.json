{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "./api/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "./dist/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "installCommand": "bun install"
}
